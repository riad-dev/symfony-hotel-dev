{% extends 'layout.html.twig' %}


{% block content %}
	<h1>Inscription</h1>
	{{ form_start(form) }}
	{{ form_row(form.nom) }}
	{{ form_row(form.email) }}
	{{ form_row(form.password) }}
	<div class="form-btn-group">
		<button type="submit" class="btn btn-primary">Enregistrer</button>
	</div>
	{{ form_end(form) }}

{% endblock %}