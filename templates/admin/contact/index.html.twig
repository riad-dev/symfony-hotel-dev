{% extends 'layout.html.twig' %}


{% block content %}
	<h1>Contact</h1>
	{{ form_start(form) }}
	{{ form_row(form.nom) }}
	{{ form_row(form.email) }}
	{{ form_row(form.telephone) }}
    {{ form_row(form.message) }}
	<div class="form-btn-group">
		<button type="submit" class="btn btn-primary">Envoyer</button>
	</div>
	{{ form_end(form) }}

{% endblock %}
